{
  "version": 2,
  "builds": [
    {
      "src": "backend/src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/src/index.ts"
    }
  ],
  "installCommand": "cd backend && npm install && cd ../curriculo-inteligente && npm install",
  "buildCommand": "cd backend && npm run vercel-build"
}